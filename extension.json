		"thomas-topway-it"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Semantic_Tasks",
	"descriptionmsg": "semantictasks-desc",
	"namemsg": "semantictasks-name",
	"license-name": "GPL-2.0-or-later",
	"type": "semantic",
	"requires": {
		"MediaWiki": ">= 1.35"
	},
	"MessagesDirs": {
		"SemanticTasks": [
			"i18n"
		]
	},
	"callback": "SemanticTasks::initExtension",
	"ExtensionFunctions": [
		"SemanticTasks::onExtensionFunction"
	],
	"AutoloadNamespaces": {
		"ST\\": "src"
	},
	"AutoloadClasses": {
		"SemanticTasks": "SemanticTasks.php"
	},
	"config_prefix": "stg",
	"config": {
		"PropertyAssignedTo": {
			"value": "Assigned to"
		},
		"PropertyCarbonCopy": {
			"value": "Carbon copy"
		},
		"PropertyTargetDate": {
			"value": "Target date"
		},
		"PropertyReminderAt": {
			"value": "Reminder at"
		},
		"PropertyStatus": {
			"value": "Status"
		},
		"PropertyAssignedToGroup": {
			"value": "Assigned to group"
		},
		"PropertyHasAssignee": {
			"value": "Has assignee"
		},
		"SemanticTasksNotifyIfUnassigned": {
			"value": false
		},
		"NotificationFromSystemAddress": {
			"value": false
		}
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
